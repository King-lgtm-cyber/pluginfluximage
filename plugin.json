{
  "id": "flux-image-generator",
  "title": "Flux Image Generator",
  "emoji": "üñºÔ∏è",
  "description": "Generate images using Flux models (dev, pro, schnell) via Fal.ai",
  "author": "King",
  "version": "1.0.0",

  "api": {
    "type": "custom"
  },

  "action": "flux_image_generator",

  "implementationType": "javascript",
  "outputType": "render_markdown",

  "userSettings": [
    {
      "name": "falKey",
      "required": true,
      "type": "password",
      "label": "Fal.ai API Key",
      "description": "Your Fal.ai API key"
    },
    {
      "name": "model",
      "type": "select",
      "label": "Flux Model",
      "options": [
        { "label": "flux-dev", "value": "flux-dev" },
        { "label": "flux-pro", "value": "flux-pro" },
        { "label": "flux-schnell", "value": "flux-schnell" }
      ],
      "defaultValue": "flux-dev"
    },
    {
      "name": "width",
      "type": "number",
      "label": "Image Width",
      "defaultValue": 1024
    },
    {
      "name": "height",
      "type": "number",
      "label": "Image Height",
      "defaultValue": 1024
    }
  ],

  "inputs": [
    {
      "id": "prompt",
      "label": "Prompt",
      "type": "text",
      "required": true
    },
    {
      "id": "negative_prompt",
      "label": "Negative Prompt",
      "type": "text"
    }
  ]
}
